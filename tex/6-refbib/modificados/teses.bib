@MastersThesis{SANTOS2007,
  Title                    = {Reestruturação urbana e movimento popular de luta por moradia},
  Author                   = {Santos, Ademir Sousa},
  School                   = {Programa de Pós-Graduação em Ciências Sociais da Faculdade de Filosofia e Ciências Humanas da Universidade Federal da Bahia (UFBA)},
  Year                     = {2007},

  Address                  = {Salvador},
  Type                     = {Mestrado em Ciências Sociais},

  Abstract                 = {Um local escondido na periferia. Um governo autoritário. Um projeto imposto. Um povo que sente medo. Um medo que vira revolta. Uma revolta que vira organização. Um grupo que luta por direitos e democratização das relações sociais... O presente trabalho tem como objetivo, a partir do desenvolvimento de uma visão sociológica, compreender a forma como a Comissão de Moradores do Dique de Campinas, se estruturou e se organizou no sentido de articular os moradores dos bairros periféricos Marechal Rondon e Alto do Cabrito, para defender seus direitos frente ao projeto de requalificação urbana do referido dique, implementado pela Prefeitura Municipal do Salvador-BA, entre os anos 2002 e 2004. Para tanto, o mesmo está divido em nove capítulos, além da introdução e das considerações finais, onde procurou-se abordar, entre outros aspectos, a localização do dique, sua construção e importância físico-sanitário e sócio-cultural; o processo de ocupação de suas margens, num contexto de luta por moradia em Salvador e de falta de políticas públicas visando a preservação dos mananciais da cidade; o projeto de requalificação urbana desse dique dentro do paradigma do planejamento estratégico e da globalização; a forma com que a PMS, com o auxílio da Embasa, tentou implementar o projeto e o processo de organização e resistência dos moradores visando garantir seus direitos de moradia e participar nas decisões do projeto. Embora se constituindo como um estudo de caso, o conflito estudado é importante porque desvela, de um lado, o modus operandi dos agentes estatais em administrar a Res Publica, e, de outro, porque é sintomático da exacerbação do conflito entre a lógica autoritária e a da participação popular, que pleiteia a participação nos destinos da cidade.},
  Pages                    = {259},
  Subtitle                 = {Organização e resistência dos moradores à implementação do projeto de requalificação urbana do {D}ique de {C}ampinas em {S}alvador, {B}ahia},
  Year-presented           = {2007}
}
