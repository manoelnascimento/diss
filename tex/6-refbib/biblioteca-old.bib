@Article{claval_allemande_1972,
  author    = {Paul Claval},
  title     = {La grande ville allemande},
  journal   = {Annales de Géographie},
  year      = {1972},
  volume    = {81},
  number    = {447},
  pages     = {538-554},
  issn      = {00034010, 17775884},
  abstract  = {Malgré la diversité qu'elles ont hérité de l'histoire, les grandes villes allemandes présentent certains caractères communs : faible digitation de l'aire construite, banlieue basse percée de très larges avenues, densité des lignes de transports en commun, prédominance des quartiers du centre en matière d'activités tertiaires. Elles les doivent à l'urbanisme technocratique de la fin du siècle dernier, au règlement d'hygiène rigoureux qu'il a imposé, à l'organisation de systèmes intégrés de transports combinant tramways, U-Bahn, S-Bahn. Depuis une quinzaine d'années, la volonté de promouvoir les centres historiques s'exprime par la réorganisation et la modernisation de ces réseaux collectifs. On assiste cependant depuis peu à l'apparition de foyers d'affaires satellites, ce qui montre la limite de l'efficacité de la politique suivie, et la transformation qui s'esquisse de nos jours. In spite of a diversity inherited from their history, the big German cities have some features in common : a little-digitated built-up area, low-housed suburbs cut by very broad avenues, a great density of transportation lines, concentration of services in the centre. Those features are due to several factors : the XIXth century's technocratic town-planning, the severe hygiene rules it edicted, the organization of transportation facilities that combine tramways, U-Bahn, S-Bahn. For fifteen years or so, those public networks have been reorganized and modernized in order to promote the historical centres. For a little time however, some satellit business centres rise, which shows the limits of efficiency of the policy followed until to-day and the transformation under way.},
  groups    = {Artigos},
  publisher = {Armand Colin},
  url       = {http://www.jstor.org.sci-hub.tw/stable/23447479},
}

